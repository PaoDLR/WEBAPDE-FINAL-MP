<div class="userPost">
    <div class="postTitle">
        <form action = "post" method="get">
            <input class="extLink" name="title" type="submit" value="<%=title%>"/>
        </form>
    </div>
    <div class="postDesc">
        <%= desc %>
    </div>
    <div class="commentsPart">
<!--        <img class="dpComments" src="comment.png"> 3 Comments-->
    </div>
    <div class="editPart">
        <form action="editPost" method="get">
         <input class ="trialEHover" type="submit" value="Edit"><img class="dpComments" src="./imgs/edit.png">
        </form>
    </div>
    <div class="deletePart">
        <% console.log(id + " <---- ID of the post")%>
        <form action="deletePost" method="get">
        <input name="id" class ="trialDHover" type="submit" value="<%=id%>"><img class="dpComments" src="./imgs/waste.png">
        </form>
    </div>
    <div class="votePart">
        <div>
            <img class="smallUp" src="./imgs/upvote.png">
        </div>
        <div>
            <img class="smallDown" src="./imgs/downvote.png">
        </div>
        <div class="voteNumbers">
            <%= likes %>
        </div>
    </div>
    <div class="dateTime">
        July 9, 2018 12:47PM
    </div>
</div>